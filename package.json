{
	"name": "binary-plist",
	"version": "0.1.0",
	"description": "Decodes binary plist files by opening a new tab with decoded XML and saving it back to binary on save.",
	"keywords": [
		"binary",
		"plist",
		"xml"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/dnicolson/binary-plist"
	},
	"license": "MIT",
	"author": {
		"name": "David Nicolson"
	},
	"main": "./out/extension.js",
	"scripts": {
		"compile": "tsc -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"lint": "tslint --fix src/**/*.ts",
		"test": "npm run compile && node ./node_modules/vscode/bin/test",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -watch -p ./"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"package.json": [
			"sort-package-json",
			"git add"
		],
		"src/**/*.ts": [
			"npm run lint",
			"git add"
		]
	},
	"dependencies": {
		"simple-plist": "^1.0.0",
		"util": "^0.12.0"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"husky": "^2.3.0",
		"lint-staged": "^8.1.7",
		"sort-package-json": "^1.22.1",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	},
	"engines": {
		"vscode": "^1.34.0"
	},
	"activationEvents": [
		"onFileSystem:plist",
		"*"
	],
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "plist",
				"extensions": [
					".plist",
					".strings"
				]
			}
		]
	},
	"displayName": "Binary Plist",
	"icon": "icon.png",
	"publisher": "dnicolson"
}
