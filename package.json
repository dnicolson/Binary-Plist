{
	"name": "binary-plist",
	"displayName": "Binary Plist",
	"version": "0.6.0",
	"description": "Decodes binary plist files by opening a new tab with decoded XML and saving it back to binary on save.",
	"categories": [
		"Programming Languages"
	],
	"keywords": [
		"binary",
		"plist",
		"xml"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/dnicolson/binary-plist"
	},
	"license": "MIT",
	"author": {
		"name": "David Nicolson"
	},
	"publisher": "dnicolson",
	"main": "./out/extension.js",
	"scripts": {
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -watch -p ./"
	},
	"contributes": {
		"languages": [
			{
				"id": "plist",
				"extensions": [
					".plist"
				]
			}
		]
	},
	"activationEvents": [
		"onFileSystem:plist",
		"*"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"package.json": [
			"sort-package-json",
			"git add"
		],
		"src/**/*.ts": [
			"npm run lint",
			"git add"
		]
	},
	"dependencies": {
		"hasbin": "^1.2.3",
		"simple-plist": "^1.1.1"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/tmp": "^0.1.0",
		"@types/vscode": "^1.59.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"husky": "^2.7.0",
		"lint-staged": "^11.0.0",
		"mocha": "^8.2.1",
		"sort-package-json": "^1.49.0",
		"tmp": "^0.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"engines": {
		"vscode": "^1.59.0"
	},
	"icon": "icon.png"
}
