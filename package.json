{
	"name": "binary-plist",
	"displayName": "Binary Plist",
	"version": "0.8.0",
	"description": "Decodes binary plist files by opening a new tab with decoded XML and saving it back to binary on save.",
	"categories": [
		"Programming Languages"
	],
	"keywords": [
		"binary",
		"plist",
		"xml"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/dnicolson/binary-plist"
	},
	"license": "MIT",
	"author": {
		"name": "David Nicolson"
	},
	"publisher": "dnicolson",
	"main": "./out/extension.js",
	"scripts": {
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"prepare": "husky install",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -watch -p ./"
	},
	"contributes": {
		"languages": [
			{
				"id": "plist",
				"extensions": [
					".plist"
				]
			}
		]
	},
	"activationEvents": [
		"onFileSystem:plist",
		"*"
	],
	"lint-staged": {
		"package.json": "sort-package-json"
	},
	"dependencies": {
		"hasbin": "^1.2.3",
		"simple-plist": "^1.3.0"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "^16.11.6",
		"@types/tmp": "^0.2.2",
		"@types/vscode": "^1.62.0",
		"@typescript-eslint/eslint-plugin": "^5.5.0",
		"@typescript-eslint/parser": "^5.0.0",
		"eslint": "^8.2.0",
		"glob": "^7.2.0",
		"husky": "^7.0.4",
		"lint-staged": "^12.1.2",
		"mocha": "^9.1.3",
		"sort-package-json": "^1.53.1",
		"tmp": "^0.2.1",
		"typescript": "^4.4.4",
		"vscode-test": "^1.5.0"
	},
	"engines": {
		"vscode": "^1.62.0"
	},
	"icon": "icon.png"
}
